    "test:contracts": "hardhat test test/TipRouter.test.ts",
    "test:backend": "echo 'Backend tests not yet implemented'",
    "test:all": "pnpm test:contracts && pnpm test:backend",
    "compile": "hardhat compile",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "deploy:testnet": "hardhat run scripts/deploy-testnet.ts --network veryTestnet",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network very",
    "demo": "node scripts/demo-run.js",
    "demo:sign": "node scripts/demo-sign.js"
  },
